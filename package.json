{
  "name": "taskforce-connector",
  "version": "1.27.0",
  "description": "Connects queues to Taskforce",
  "preferGlobal": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "lib/index.ts",
  "bin": {
    "taskforce": "index.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "bull": "^4.12.6",
    "bullmq": "^5.7.9",
    "chalk": "^4.1.0",
    "commander": "^5.1.0",
    "ioredis": "^5.4.1",
    "latest-version": "^5.1.0",
    "lodash": "^4.17.21",
    "semver": "^7.3.2",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.1.1",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@types/bull": "^3.15.8",
    "@types/chalk": "^2.2.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^20.10.5",
    "@types/ws": "^6.0.1",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "./index.js",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taskforcesh/taskforce-connector.git"
  },
  "keywords": [
    "taskforce",
    "bull",
    "queue"
  ],
  "author": "Taskforce.sh Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taskforcesh/taskforce-connector/issues"
  },
  "homepage": "https://github.com/taskforcesh/taskforce-connector#readme",
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "yarn.lock",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
